{
  "type": "mysql",
  "host": "[HOST_DATABASE]",
  "port": [PORT_DATABASE],
  "username": "[USER_DATABASE]",
  "password": "[PASS_DATABASE]",
  "database": "[NAME_DATABASE]",
  "entities": ["dist/**/*.model.js"],
  "synchronize": true,
  "logging": true,
  "migrations": ["dist/migrations/**/*.js"],
  "cli": {
    "migrationsDir": "migrations"
  }
}
